<query id="insertItem" action="insert">
    <tables>
        <table name="resource_items" />
    </tables>
    <columns>
        <column name="module_srl" var="module_srl" filter="number" notnull="notnull" />
        <column name="package_srl" var="package_srl" filter="number" notnull="notnull" />
        <column name="item_srl" var="item_srl" notnull="notnull" />
        <column name="version" var="version" notnull="notnull" />
        <column name="downloaded" default="0" />
        <column name="voted" default="0" />
        <column name="voter" default="0" />
        <column name="document_srl" var="document_srl" notnull="notnull" />
        <column name="regdate" default="curdate()" notnull="notnull" />
        <column name="list_order" var="list_order" notnull="notnull" />
    </columns>
</query>
