<?xml version="1.0" encoding="UTF-8"?>
<query id="getFavouriteOfMe" action="select">
    <tables>
        <table name="xewall_favourite"/>
    </tables>
    <columns>
    	<column name="*"/>
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" />
    </conditions>
    <navigation>
        <index var="sort_index" default="favourite_srl" order="asc" />
    </navigation>
</query>